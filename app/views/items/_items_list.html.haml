%table.table
  %thead
    %tr
      %td Item name
      %td Category
      %td Purchase location
      %td Purchase date
      %td Price
  %tbody
    - obj.each do |item|
      %tr
        %td= link_to item.name, item
        %td= link_to item.category.name, item.category
        %td= item.purchase_location
        %td= pretty_date(item.purchase_date)  
        %td= pretty_price(item.price)